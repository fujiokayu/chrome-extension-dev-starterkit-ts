{
  "manifest_version": 3,
  "name": "Sample Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "cat.ico"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [ "content_styles.css" ],
      "js": [ "content_scripts.js" ]
    }
  ]
}
